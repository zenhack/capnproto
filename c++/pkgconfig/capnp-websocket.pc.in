prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: Cap'n Proto RPC Over WebSockets
Description: WebSocket transport for Cap'n Proto RPC.
Version: @VERSION@
Libs: -L${libdir} -lcapnp-rpc -lkj-http @PTHREAD_CFLAGS@ @PTHREAD_LIBS@ @STDLIB_FLAG@
Requires: kj-http capnp-rpc = @VERSION@
Cflags: -I${includedir} @PTHREAD_CFLAGS@ @STDLIB_FLAG@ @CAPNP_LITE_FLAG@
